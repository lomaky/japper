{
   "extends": ["tslint:recommended", "tslint-config-prettier"],
   "rules": {
        "no-console": false,
        "no-var-requires": false,
        "no-empty": false,
        "no-shadowed-variable": false,
        "class-name": false,
        "variable-name": {
            "options": [
                "ban-keywords",
                "check-format",
                "allow-leading-underscore",
                "allow-pascal-case"
            ]
        },
        "prefer-const": "off"
    }
}